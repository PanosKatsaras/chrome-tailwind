{
  "name": "chrome",
  "version": "1.0.0",
  "description": "Chrome UI Clone",
  "homepage": "https://PanosKatsaras.github.io/chrome-tailwind",
  "scripts": {
    "watch": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "start": "npx live-server",
    "dev": "npm run watch & npm start",
    "predeploy": "npm run build",
    "build": "npx tailwindcss -i ./src/input.css -o ./src/output.css",
    "deploy": "gh-pages -d ."
  },
  "keywords": [
    "chrome",
    "ui",
    "tailwind"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^6.2.0",
    "live-server": "^1.2.2",
    "tailwindcss": "^3.4.17"
  }
}
